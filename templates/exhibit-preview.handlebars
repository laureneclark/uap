<div class="exhibit-preview panel panel primary" data-exhibit-id={{_id}}>
<div class="panel-heading">
	<h1 class="panel-title"> {{title}}</h1>
</div>
<div class = "panel-body">
	<h5>{{dateStart}}</h5>
	<h5>{{dateEnd}}</h5>
	<h5>{{location}}</h5>
	<button class="btn view-exhibit-btn" id={{_id}}> View </button>
</div>

